@model IEnumerable<LibraryWebApplication1.Stadium>

@{
    ViewData["Title"] = "Запит 7";
}

<h1>Запит 7</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StName)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StName)
            </td>
        </tr>
}
    </tbody>
</table>
